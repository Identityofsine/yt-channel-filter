{
  "name": "Youtube Channel Filter",
  "description": "Become more productive by filtering out channels.",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions":["storage", "activeTab", "scripting", "tabs"],
  "host_permissions": [
    "https://www.youtube.com/",
    "https://*.youtube.com/"
  ],
  "content_scripts": [
    {
      "matches": ["https://*.youtube.com/*"],
      "css": ["ytcss.css"],
      "js": ["ytscript.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  }
}